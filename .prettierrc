{
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"endOfLine": "lf",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrder": ["^@/(.*)$", "^[./]"]
}
